[Unit]
Description=Start VPN service to access Oracle Intranet

[Service]
ExecStart=/bin/bash -c 'exec /usr/sbin/openconnect --verbose --cafile=/etc/ssl/certs/ca-certificates.crt --user=fmorcos_at --passwd-on-stdin --script-tun --script "/home/fred/.local/bin/tunsocks -D 127.0.0.1:8472 -R ssh" myaccess.oraclevpn.com < ~/Documents/Important/Passwords/ov.txt'
# Or ams-twvpn-1a.oraclevpn.com
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
